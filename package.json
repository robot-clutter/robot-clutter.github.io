{
  "name": "robot-clutter.github.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "lint": "eslint --fix .",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc.js",
    "clean": "npm run format && npm run lint"
  },
  "dependencies": {
    "next": "10.0.5",
    "next-themes": "0.0.10",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-player": "2.7.2",
    "react-toggle-dark-mode": "1.0.3",
    "tailwindcss": "2.0.2"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "3.1.3",
    "@tailwindcss/aspect-ratio": "0.2.0",
    "@tailwindcss/typography": "0.3.1",
    "@types/node": "14.14.20",
    "@types/react": "17.0.0",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "@typescript-eslint/parser": "4.12.0",
    "autoprefixer": "10.2.1",
    "cssnano": "4.1.10",
    "eslint": "7.17.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.7",
    "postcss": "8.2.4",
    "postcss-100vh-fix": "1.0.2",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-hover-media-feature": "1.0.0",
    "postcss-import": "14.0.0",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "typescript": "4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run format"
    }
  }
}
